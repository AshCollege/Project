<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.example.Objects.ClientObject" table="clients" lazy="false">
        <id name="oid" type="int" column="oid">
            <generator class="native"/>
        </id>
        <property name="name" column="name" type="string"/>
        <property name="addedDate" column="added_date" type="timestamp"/>
        <property name="lastTransactionDate" column="last_transaction_date" type="timestamp"/>
        <property name="debt" column="debt" type="double"/>
        <property name="active" column="active" type="boolean"/>
    </class>

    <class name="com.example.Objects.HishgadPackageObject" table="hishgad_packages" lazy="false">
        <id name="oid" type="int" column="oid">
            <generator class="native"/>
        </id>
        <property name="name" column="name" type="string"/>
        <property name="ticketPrice" column="ticket_price" type="int"/>
        <property name="ticketsAmount" column="tickets_amount" type="int"/>
        <property name="commission" column="commission" type="double"/>
        <property name="addedDate" column="added_date" type="timestamp"/>
        <property name="remaining" column="remaining" type="int"/>
        <property name="active" column="active" type="boolean"/>
    </class>

    <class name="com.example.Objects.OrderObject" table="orders" lazy="false">
        <id name="oid" type="int" column="oid">
            <generator class="native"/>
        </id>
        <property name="price" column="price" type="int"/>
        <property name="priceWithFee" column="price_with_fee" type="int"/>
        <property name="purchaseDate" column="purchase_date" type="timestamp"/>
        <property name="payDate" column="pay_date" type="timestamp"/>
        <set name="orderPackageObjectSet" table="order_packages"
             inverse="true" lazy="false" fetch="select">
            <key>
                <column name="hishgad_package_oid" not-null="true" />
            </key>
            <one-to-many class="com.example.Objects.OrderPackageObject" />
        </set>
        <many-to-one name="lottoPaymentObject" column="lotto_payment_oid"
                     class="com.example.Objects.LottoPaymentObject"/>
    </class>

    <class name="com.example.Objects.OrderPackageObject" table="order_packages" lazy="false">
        <id name="oid" type="int" column="oid">
            <generator class="native"/>
        </id>
        <many-to-one name="orderObject" column="order_oid"
                     class="com.example.Objects.OrderObject"/>
        <many-to-one name="hishgadPackageObject" column="hishgad_package_oid"
                     class="com.example.Objects.HishgadPackageObject"/>
        <property name="amount" column="amount" type="int"/>
    </class>

    <class name="com.example.Objects.DailySalesObject" table="daily_sales" lazy="false">
        <id name="oid" type="int" column="oid">
            <generator class="native"/>
        </id>
        <property name="date" column="date" type="timestamp"/>
        <property name="lottoSales" column="lotto_sales" type="double"/>
        <property name="lottoWins" column="lotto_wins" type="double"/>
        <property name="extraSales" column="extra_sales" type="double"/>
        <property name="extraWins" column="extra_wins" type="double"/>
        <property name="specialSales" column="special_salees" type="double"/>
        <property name="specialWins" column="special_wins" type="double"/>
        <property name="chanceSales" column="chance_sales" type="double"/>
        <property name="chanceWins" column="chance_wins" type="double"/>
        <property name="oneTwoThreeSales" column="one_two_three_sales" type="double"/>
        <property name="oneTwoThreeWins" column="one_two_three_wins" type="double"/>
        <property name="sevenSales" column="seven_sales" type="double"/>
        <property name="sevenWins" column="seven_wins" type="double"/>
        <property name="hishgadWins" column="hishgad_wins" type="double"/>
        <many-to-one name="lottoPaymentObject" column="lotto_payment_oid"
                     class="com.example.Objects.LottoPaymentObject"/>
    </class>

    <class name="com.example.Objects.HishgadDailySalesObject" table="hishgad_daily_sales" lazy="false">
        <id name="oid" type="int" column="oid">
            <generator class="native"/>
        </id>
        <property name="date" column="date" type="timestamp"/>
        <property name="sum" column="sum" type="double"/>
    </class>

    <class name="com.example.Objects.DailyDebtObject" table="daily_debts" lazy="false">
        <id name="oid" type="int" column="oid">
            <generator class="native"/>
        </id>
        <property name="date" column="date" type="timestamp"/>
        <property name="beforeSum" column="before_sum" type="double"/>
        <property name="afterSum" column="after_sum" type="double"/>
        <many-to-one name="clientObject" column="client_oid"
                     class="com.example.Objects.ClientObject"/>
    </class>

    <class name="com.example.Objects.DepositObject" table="deposits" lazy="false">
        <id name="oid" type="int" column="oid">
            <generator class="native"/>
        </id>
        <property name="date" column="date" type="timestamp"/>
        <property name="amountCash" column="amount_cash" type="double"/>
        <property name="amountCheck" column="amount_check" type="double"/>
        <property name="cashComment" column="cash_comment" type="string"/>
        <property name="checkComment" column="check_comment" type="string"/>
    </class>

    <class name="com.example.Objects.IncomeOutcomeObject" table="income_outcome" lazy="false">
        <id name="oid" type="int" column="oid">
            <generator class="native"/>
        </id>
        <property name="date" column="date" type="timestamp"/>
        <property name="income" column="income" type="double"/>
        <property name="outcome" column="outcome" type="double"/>
        <property name="details" column="details" type="string"/>
    </class>

    <class name="com.example.Objects.BankIncomeOutcomeObject" table="Bank_income_outcome" lazy="false">
        <id name="oid" type="int" column="oid">
            <generator class="native"/>
        </id>
        <property name="date" column="date" type="timestamp"/>
        <property name="income" column="income" type="double"/>
        <property name="outcome" column="outcome" type="double"/>
        <property name="details" column="details" type="string"/>
    </class>


    <class name="com.example.Objects.DailySalaryObject" table="daily_salary" lazy="false">
        <id name="oid" type="int" column="oid">
            <generator class="native"/>
        </id>
        <property name="date" column="date" type="timestamp"/>
        <property name="lottoFee" column="lotto_fee" type="double"/>
        <property name="hishgadFee" column="hishgad_fee" type="double"/>
        <property name="hishgadPurchaseFee" type="double">
            <column name="hishgad_purchase_fee" default="0"/>
        </property>


    </class>

    <class name="com.example.Objects.CreditObject" table="daily_credit" lazy="false">
        <id name="oid" type="int" column="oid">
            <generator class="native"/>
        </id>
        <property name="date" column="date" type="timestamp"/>
        <property name="cuponsSum" column="cupon_sum" type="double"/>
        <many-to-one name="lottoPaymentObject" column="lotto_payment_oid"
                     class="com.example.Objects.LottoPaymentObject"/>
    </class>

    <class name="com.example.Objects.CreditRefundObject" table="daily_credit_refund" lazy="false">
        <id name="oid" type="int" column="oid">
            <generator class="native"/>
        </id>
        <property name="date" column="date" type="timestamp"/>
        <property name="refundSum" column="refund_sum" type="double"/>
        <many-to-one name="lottoPaymentObject" column="lotto_payment_oid"
                     class="com.example.Objects.LottoPaymentObject"/>
    </class>

    <class name="com.example.Objects.DailySummaryObject" table="daily_summary" lazy="false">
        <id name="oid" type="int" column="oid">
            <generator class="native"/>
        </id>
        <property name="date" column="date" type="timestamp"/>
        <property name="startAmount" column="start_amount" type="double"/>
        <property name="comment" column="comment" type="string"/>
    </class>

    <class name="com.example.Objects.TranslationObject" table="translations" lazy="false">
        <id name="oid" type="int" column="oid">
            <generator class="native"/>
        </id>
        <property name="translationKey" column="translation_key" type="string"/>
        <property name="translationValue" column="translation_value" type="string"/>
    </class>

    <class name="com.example.Objects.BalanceObject" table="balance" lazy="false">
        <id name="oid" type="int" column="oid">
            <generator class="native"/>
        </id>
        <property name="date" column="date" type="timestamp"/>
        <property name="totalEquity" column="total_equity" type="double"/>
        <property name="balance" column="balance" type="double"/>
        <property name="cash" column="cash" type="double"/>
        <property name="hishgadEquity" column="hishgad_equity" type="double"/>
        <property name="deposit" column="deposit" type="double"/>
        <property name="hishgadPayment" column="hishgad_payment" type="double"/>
        <property name="lottoPayment" column="lotto_payment" type="double"/>
        <property name="creditCupons" column="credit_cupons" type="double"/>
        <property name="creditRefund" column="credit_refund" type="double"/>
        <property name="creditPending" column="credit_pending" type="double"/>
        <property name="hishgadPurchase" column="hishgad_purchase" type="double"/>
        <property name="lottoSales" column="lotto_sales" type="double"/>
        <property name="lottoWins" column="lotto_wins" type="double"/>
        <property name="hishgadSales" column="hishgad_sales" type="double"/>
        <property name="cashIncome" column="cash_income" type="double"/>
        <property name="cashOutcome" column="cash_outcome" type="double"/>
        <property name="bankIncome" column="bank_income" type="double"/>
        <property name="bankOutcome" column="bank_outcome" type="double"/>
        <property name="previousTotalEquity" column="previous_total_equity" type="double"/>
        <property name="previousBalance" column="previous_balance" type="double"/>
        <property name="previousCash" column="previous_cash" type="double"/>
        <property name="previousHishgadEquity" column="previous_hishgad_equity" type="double"/>
    </class>

    <class name="com.example.Objects.LottoPaymentObject" table="lotto_payments" lazy="false">
        <id name="oid" type="int" column="oid">
            <generator class="native"/>
        </id>
        <property name="date" column="date" type="timestamp"/>
        <property name="total" column="total" type="double"/>
        <property name="totalRefunds" column="total_refunds" type="double"/>
        <property name="totalReports" column="total_reports" type="double"/>
        <property name="totalOrders" column="total_orders" type="double"/>
        <property name="others" column="others" type="double"/>
    </class>

    <class name="com.example.Objects.ConfigObject" table="config">
        <id name="oid" type="int" column="oid">
            <generator class="native"/>
        </id>
        <property name="configKey" type="string" column="config_key"/>
        <property name="configValue" type="string" column="config_value"/>
    </class>

    <class name="com.example.Objects.CashDetailsObject" table="cash_details" lazy="false">
        <id name="oid" type="int" column="oid">
            <generator class="native"/>
        </id>
        <property name="date" column="date" type="timestamp"/>
        <property name="amount200" column="amount_200" type="int"/>
        <property name="amount100" column="amount_100" type="int"/>
        <property name="amount50" column="amount_50" type="int"/>
        <property name="amount20" column="amount_20" type="int"/>
        <property name="amount10" column="amount_10" type="int"/>
        <property name="amount5" column="amount_5" type="int"/>
        <property name="amount1" column="amount_1" type="double"/>
        <property name="amountChecks" column="amount_check" type="double"/>
    </class>

    <class name="com.example.Objects.ExceptionObject" table="exceptions" lazy="false">
        <id name="oid" type="int" column="oid">
            <generator class="native"/>
        </id>
        <property name="date" column="date" type="timestamp"/>
        <property name="className" type="string" column="class_name"/>
        <property name="methodName" type="string" column="method_name"/>
        <property name="exceptionName" type="string" column="exception_name"/>
        <property name="exceptionDetails" type="string" column="exception_details"/>
        <property name="stackTrace" type="string" column="stack_trace"/>
        <property name="emailSent" type="string" column="email_sent"/>
    </class>


    <class name="com.example.Objects.UserObject" table="users" lazy="false">
        <id name="uid" type="int" column="uid">
            <generator class="native"/>
        </id>
        <property name="name" column="name" type="string"/>
        <property name="userName" column="username" type="string"/>
        <property name="password" column="password" type="string"/>
        <property name="lastLogin" column="last_login" type="timestamp"/>
        <property name="lastPracticeDate" column="last_practice_date" type="timestamp"/>
        <many-to-one name="currentSubject" column="subject_oid"
                     class="com.example.Objects.SubjectObject"/>
        <property name="userType" column="user_type" type="int"/>
        <many-to-one name="supervisor" column="supervisor_uid"
                     class="com.example.Objects.UserObject"/>
        <property name="active" type="boolean">
            <column name="active" default="false"/>
        </property>
    </class>

    <class name="com.example.Objects.SubjectObject" table="subjects" lazy="false">
        <id name="oid" type="int" column="oid">
            <generator class="native"/>
        </id>
        <property name="name" type="string" column="name"/>
        <property name="description" column="description" type="string"/>
        <property name="priority" column="priority" type="int"/>
        <property name="link" column="link" type="string"/>
    </class>


    <class name="com.example.Objects.UserScoreObject" table="users_scores">
        <id name="oid" type="int" column="oid">
            <generator class="native"/>
        </id>
        <property name="uid" type="int" column="uid"/>
        <property name="score" column="score" type="long"/>
    </class>

    <class name="com.example.Objects.TutorObject" table="tutors">
        <id name="oid" type="int" column="oid">
            <generator class="native"/>
        </id>
        <property name="name" type="string" column="name"/>
        <property name="telephoneNumber" column="telephone_number" type="string"/>
        <property name="email" type="string" column="email"/>
        <property name="pricePerHour" type="int">
            <column name="price_per_hour" default="0"/>
        </property>
        <property name="activePayment" type="boolean" column="active_payment"/>
    </class>

    <class name="com.example.Objects.UserQuestionObject" table="user_questions">
        <id name="oid" type="int" column="oid">
            <generator class="native"/>
        </id>
        <many-to-one name="user" column="uid"
                     class="com.example.Objects.UserObject" not-null="true"/>
        <many-to-one name="subject" column="subject_oid"
                     class="com.example.Objects.SubjectObject" not-null="true"/>
        <property name="level" type="int" column="level"/>
        <property name="questionType" type="int" column="question_type"/>
        <property name="success" type="boolean" column="success"/>
    </class>

    <class name="com.example.Objects.ExampleObject" table="names">
        <id name="oid" type="int" column="oid">
            <generator class="native"/>
        </id>
        <property name="name" type="string" column="name"/>
        <property name="male" type="boolean" column="male"/>
    </class>

    <class name="com.example.Objects.ExplanationVideoObject" table="explanation_videos">
        <id name="oid" type="int" column="oid">
            <generator class="native"/>
        </id>
        <many-to-one name="requestingUser" column="uid"
                     class="com.example.Objects.UserObject" not-null="true"/>
        <many-to-one name="subject" column="subject_oid"
                     class="com.example.Objects.SubjectObject" not-null="true"/>
        <property name="name" type="string" column="name"/>
        <property name="questionText" type="string" column="question_text"/>
        <property name="requestTime" type="timestamp" column="request_time"/>
        <property name="uploadTime" type="timestamp" column="upload_time"/>
        <property name="uploaded" type="boolean" column="uploaded"/>
    </class>

    <class name="com.example.Objects.SentMessageObject" table="sent_messages">
        <id name="oid" type="int" column="oid">
            <generator class="native"/>
        </id>
        <many-to-one name="sender" column="sender_uid"
                     class="com.example.Objects.UserObject" not-null="true"/>
        <property name="title" type="string" column="title"/>
        <property name="details" type="string" column="details"/>
        <property name="sendingDate" type="timestamp" column="sending_date"/>
        <property name="messageRead" type="boolean" column="message_read"/>
    </class>

    <class name="com.example.Objects.UserSubjectLevelObject" table="user_subjects_levels">
        <id name="oid" type="int" column="oid">
            <generator class="native"/>
        </id>
        <many-to-one name="user" column="uid"
                     class="com.example.Objects.UserObject" not-null="true"/>
        <many-to-one name="subject" column="subject_oid"
                     class="com.example.Objects.SubjectObject" not-null="true"/>
        <property name="level" column="level" type="int"/>

    </class>

    <class name="com.example.Objects.HishgadDailyTraceObject" table="hishgad_daily_trace" lazy="false">
        <id name="oid" type="int" column="uid">
            <generator class="native"/>
        </id>
        <property name="beforeSum" column="before_sum" type="int"/>
        <property name="afterSum" column="after_sum" type="int"/>
        <property name="date" column="date" type="timestamp"/>
        <many-to-one name="hishgadPackageObject" column="hishgad_package_oid"
                     class="com.example.Objects.HishgadPackageObject"/>
        <many-to-one name="hishgadDailySalesObject" column="hishgad_daily_sales_oid"
                     class="com.example.Objects.HishgadDailySalesObject"/>
    </class>

    <class name="com.example.Objects.MessageObject" table="messages" lazy="false">
        <id name="oid" type="int" column="oid">
            <generator class="native"/>
        </id>
        <property name="creationDate" column="creation_date" type="timestamp"/>
        <property name="expirationDate" column="expiration_date" type="timestamp"/>
        <property name="message" column="message" type="string"/>
        <property name="active" column="active" type="boolean"/>
        <property name="comment" column="comment" type="string"/>
        <property name="type" column="type" type="int"/>
        <property name="backgroundColor" column="background_color" type="int" not-null="true"/>
    </class>

</hibernate-mapping>
